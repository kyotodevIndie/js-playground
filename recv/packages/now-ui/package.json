{
  "name": "now-ui",
  "version": "0.0.2",
  "description": "A Ui kit for NowApp and others Apps UseReserva",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=16",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwind-merge": "^1.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "kyotodevIndie",
  "module": "dist/awesome-component-library.esm.js",
  "size-limit": [
    {
      "path": "dist/awesome-component-library.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/awesome-component-library.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addons": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "husky": "^8.0.3",
    "postcss": "^8.4.28",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.6",
    "storybook": "^7.4.0",
    "tailwindcss": "^3.3.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.22.11",
    "@phosphor-icons/react": "^2.0.10",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwind-merge": "^1.14.0"
  }
}